mvn -B -f ../Generator/pom.xml -Dmaven.test.skip=true -DskipTests clean eclipse:eclipse xtend:compile install
mvn -B -Dmaven.test.skip=true -DskipTests clean eclipse:eclipse xtend:compile install org.apache.maven.plugins:maven-dependency-plugin:copy-dependencies
mvn -B xtend:testCompile test
