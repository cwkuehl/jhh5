<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.cwkuehl.jhh6.app.controller.wp.WP310KonfigurationController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%WP310.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%WP310.nr.tt" promptText="%WP310.nr.tt" GridPane.columnIndex="1"
		GridPane.columnSpan="3" GridPane.hgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%WP310.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="bezeichnung0" mnemonicParsing="true" text="%WP310.bezeichnung" GridPane.columnIndex="0"
		GridPane.rowIndex="1" />
	<TextField fx:id="bezeichnung" accessibleText="%WP310.bezeichnung.tt" promptText="%WP310.bezeichnung.tt" GridPane.columnIndex="1"
		GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%WP310.bezeichnung.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="box0" mnemonicParsing="true" text="%WP310.box" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<TextField fx:id="box" accessibleText="%WP310.box.tt" promptText="%WP310.box.tt"
		GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%WP310.box.tt" />
		</tooltip>
	</TextField>
	<ComboBox fx:id="skala" accessibleText="%WP310.skala.tt" GridPane.columnIndex="3"
		GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%WP310.skala.tt" />
		</tooltip>
	</ComboBox>
	<Label fx:id="umkehr0" mnemonicParsing="true" text="%WP310.umkehr" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextField fx:id="umkehr" accessibleText="%WP310.umkehr.tt" promptText="%WP310.umkehr.tt"
		GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%WP310.umkehr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="methode0" mnemonicParsing="true" text="%WP310.methode" GridPane.columnIndex="0" GridPane.rowIndex="4" />
	<ComboBox fx:id="methode" accessibleText="%WP310.methode.tt" GridPane.columnIndex="1"
		GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4">
		<tooltip>
			<Tooltip text="%WP310.methode.tt" />
		</tooltip>
	</ComboBox>
	<Label fx:id="dauer0" mnemonicParsing="true" text="%WP310.dauer" GridPane.columnIndex="0" GridPane.rowIndex="5" />
	<TextField fx:id="dauer" accessibleText="%WP310.dauer.tt" promptText="%WP310.dauer.tt"
		GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5">
		<tooltip>
			<Tooltip text="%WP310.dauer.tt" />
		</tooltip>
	</TextField>
	<CheckBox fx:id="relativ" accessibleText="%WP310.relativ.tt" text="%WP310.relativ"
		GridPane.columnIndex="3" GridPane.columnSpan="1" GridPane.rowIndex="5">
		<tooltip>
			<Tooltip text="%WP310.relativ.tt" />
		</tooltip>
	</CheckBox>
	<Label fx:id="status0" mnemonicParsing="true" text="%WP310.status" GridPane.columnIndex="0" GridPane.rowIndex="6" />
	<ComboBox fx:id="status" accessibleText="%WP310.status.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="6">
		<tooltip>
			<Tooltip text="%WP310.status.tt" />
		</tooltip>
	</ComboBox>
	<Label fx:id="notiz0" text="%WP310.notiz" GridPane.columnIndex="0" GridPane.rowIndex="7" />
	<TextArea fx:id="notiz" accessibleText="%WP310.notiz.tt" promptText="%WP310.notiz.tt" wrapText="true" GridPane.columnIndex="1"
		GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="7" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%WP310.notiz.tt" />
		</tooltip>
	</TextArea>
	<Label fx:id="angelegt0" text="%Creation" GridPane.columnIndex="0" GridPane.rowIndex="8" />
	<TextField fx:id="angelegt" accessibleText="%Creation.tt"
		promptText="%Creation.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="8">
		<tooltip>
			<Tooltip text="%Creation.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%Change" GridPane.columnIndex="0" GridPane.rowIndex="9" />
	<TextField fx:id="geaendert" accessibleText="%Change.tt"
		promptText="%Change.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="9">
		<tooltip>
			<Tooltip text="%Change.tt" />
		</tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="10">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
</GridPane>
