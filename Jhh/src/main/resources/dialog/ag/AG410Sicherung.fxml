<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.cwkuehl.jhh6.app.controller.ag.AG410SicherungController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%AG410.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%AG410.nr.tt" promptText="%AG410.nr.tt" GridPane.columnIndex="1"
		GridPane.hgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%AG410.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="ziel0" mnemonicParsing="true" text="%AG410.ziel" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="ziel" accessibleText="%AG410.ziel.tt" promptText="%AG410.ziel.tt" GridPane.columnIndex="1"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%AG410.ziel.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="quelle0" mnemonicParsing="true" text="%AG410.quelle" GridPane.columnIndex="0"
		GridPane.rowIndex="2" />
	<TextField fx:id="quelle" accessibleText="%AG410.quelle.tt" promptText="%AG410.quelle.tt"
		GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%AG410.quelle.tt" />
		</tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
</GridPane>
