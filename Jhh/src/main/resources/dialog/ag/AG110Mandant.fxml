<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.cwkuehl.jhh6.app.controller.ag.AG110MandantController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%AG110.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%AG110.nr.tt" promptText="%AG110.nr.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%AG110.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="beschreibung0" mnemonicParsing="true" text="%AG110.beschreibung" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="beschreibung" accessibleText="%AG110.beschreibung.tt" promptText="%AG110.beschreibung.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%AG110.beschreibung.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="angelegt0" text="%AG110.angelegt" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<TextField fx:id="angelegt" accessibleText="%AG110.angelegt.tt" promptText="%AG110.angelegt.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%AG110.angelegt.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%AG110.geaendert" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextField fx:id="geaendert" accessibleText="%AG110.geaendert.tt" promptText="%AG110.geaendert.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%AG110.geaendert.tt" />
		</tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
