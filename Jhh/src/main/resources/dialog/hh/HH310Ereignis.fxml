<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.cwkuehl.jhh6.app.controller.hh.HH310EreignisController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%HH310.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%HH310.nr.tt" promptText="%HH310.nr.tt" GridPane.columnIndex="1" GridPane.columnSpan="3">
		<tooltip><Tooltip text="%HH310.nr.tt" /></tooltip>
	</TextField>
	<Label fx:id="bezeichnung0" mnemonicParsing="true" text="%HH310.bezeichnung" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="bezeichnung" accessibleText="%HH310.bezeichnung.tt" promptText="%HH310.bezeichnung.tt" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1">
		<tooltip><Tooltip text="%HH310.bezeichnung.tt" /></tooltip>
	</TextField>
	<Label fx:id="kennzeichen0" mnemonicParsing="true" text="%HH310.kennzeichen" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<TextField fx:id="kennzeichen" accessibleText="%HH310.kennzeichen.tt" promptText="%HH310.kennzeichen.tt" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="2">
		<tooltip><Tooltip text="%HH310.kennzeichen.tt" /></tooltip>
	</TextField>
	<Label fx:id="eText0" mnemonicParsing="true" text="%HH310.eText" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextField fx:id="eText" accessibleText="%HH310.eText.tt" promptText="%HH310.eText.tt" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="3">
		<tooltip><Tooltip text="%HH310.eText.tt" /></tooltip>
	</TextField>
	<Label fx:id="sollkonto0" mnemonicParsing="true" text="%HH310.sollkonto" GridPane.columnIndex="0" GridPane.rowIndex="4" />
	<ListView fx:id="sollkonto" accessibleText="%HH310.sollkonto.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
		<tooltip><Tooltip text="%HH310.sollkonto.tt" /></tooltip>
	</ListView>
	<Label fx:id="habenkonto0" mnemonicParsing="true" text="%HH310.habenkonto" GridPane.columnIndex="2" GridPane.rowIndex="4" />
	<ListView fx:id="habenkonto" accessibleText="%HH310.habenkonto.tt" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
		<tooltip><Tooltip text="%HH310.habenkonto.tt" /></tooltip>
	</ListView>
	<Label fx:id="angelegt0" text="%Creation" GridPane.columnIndex="0" GridPane.rowIndex="5" />
	<TextField fx:id="angelegt" accessibleText="%Creation.tt" promptText="%Creation.tt" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="5">
		<tooltip><Tooltip text="%Creation.tt" /></tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%Change" GridPane.columnIndex="0" GridPane.rowIndex="6" />
	<TextField fx:id="geaendert" accessibleText="%Change.tt" promptText="%Change.tt" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="6">
		<tooltip><Tooltip text="%Change.tt" /></tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="7" GridPane.columnSpan="3">
	<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
		<tooltip><Tooltip text="%Ok.tt" /></tooltip>
	</Button>
	<Button fx:id="kontentausch" mnemonicParsing="true" onAction="#onKontentausch" text="%HH310.kontentausch">
		<tooltip><Tooltip text="%HH310.kontentausch.tt" /></tooltip>
	</Button>
	<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
		<tooltip><Tooltip text="%Cancel.tt" /></tooltip>
	</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
   <columnConstraints>
      <ColumnConstraints minWidth="105.0" />
      <ColumnConstraints />
      <ColumnConstraints minWidth="90.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
