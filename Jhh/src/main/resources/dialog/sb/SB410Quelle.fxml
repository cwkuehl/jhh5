<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.cwkuehl.jhh6.app.controller.sb.SB410QuelleController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%SB410.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%SB410.nr.tt" promptText="%SB410.nr.tt" GridPane.columnIndex="1"
		GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB410.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="autor0" mnemonicParsing="true" text="%SB410.autor" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="autor" accessibleText="%SB410.autor.tt" promptText="%SB410.autor.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
		GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%SB410.autor.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="beschreibung0" mnemonicParsing="true" text="%SB410.beschreibung" GridPane.columnIndex="0"
		GridPane.rowIndex="2" />
	<TextField fx:id="beschreibung" accessibleText="%SB410.beschreibung.tt" promptText="%SB410.beschreibung.tt"
		GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%SB410.beschreibung.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="zitat0" text="%SB410.zitat" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextArea fx:id="zitat" accessibleText="%SB410.zitat.tt" promptText="%SB410.zitat.tt" wrapText="true" GridPane.columnIndex="1"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB410.zitat.tt" />
		</tooltip>
	</TextArea>
	<Label fx:id="bemerkung0" text="%SB410.bemerkung" GridPane.columnIndex="0" GridPane.rowIndex="4" />
	<TextArea fx:id="bemerkung" accessibleText="%SB410.bemerkung.tt" promptText="%SB410.bemerkung.tt" wrapText="true"
		GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB410.bemerkung.tt" />
		</tooltip>
	</TextArea>
	<Label fx:id="angelegt0" text="%Creation" GridPane.columnIndex="0" GridPane.rowIndex="5" />
	<TextField fx:id="angelegt" accessibleText="%Creation.tt"
		promptText="%Creation.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
		GridPane.rowIndex="5">
		<tooltip>
			<Tooltip text="%Creation.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%Change" GridPane.columnIndex="0" GridPane.rowIndex="6" />
	<TextField fx:id="geaendert" accessibleText="%Change.tt"
		promptText="%Change.tt" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
		GridPane.rowIndex="6">
		<tooltip>
			<Tooltip text="%Change.tt" />
		</tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="7">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
</GridPane>
