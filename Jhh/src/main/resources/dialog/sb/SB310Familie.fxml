<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.cwkuehl.jhh6.app.controller.sb.SB310FamilieController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%SB310.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%SB310.nr.tt" promptText="%SB310.nr.tt" GridPane.columnIndex="1"
		GridPane.columnSpan="3" GridPane.hgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB310.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="vater0" mnemonicParsing="true" text="%SB310.vater" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<ComboBox fx:id="vater" accessibleText="%SB310.vater.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%SB310.vater.tt" />
		</tooltip>
	</ComboBox>
	<Label fx:id="mutter0" mnemonicParsing="true" text="%SB310.mutter" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<ComboBox fx:id="mutter" accessibleText="%SB310.mutter.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%SB310.mutter.tt" />
		</tooltip>
	</ComboBox>
	<Label fx:id="heiratsdatum0" text="%SB310.heiratsdatum" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextField fx:id="heiratsdatum" accessibleText="%SB310.heiratsdatum.tt" promptText="%SB310.heiratsdatum.tt"
		GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%SB310.heiratsdatum.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="heiratsort0" text="%SB310.heiratsort" GridPane.columnIndex="2" GridPane.rowIndex="3" />
	<TextField fx:id="heiratsort" accessibleText="%SB310.heiratsort.tt" promptText="%SB310.heiratsort.tt" GridPane.columnIndex="3"
		GridPane.columnSpan="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%SB310.heiratsort.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="heiratsbem0" text="%SB310.heiratsbem" GridPane.columnIndex="0" GridPane.rowIndex="4" />
	<TextArea fx:id="heiratsbem" accessibleText="%SB310.heiratsbem.tt" promptText="%SB310.heiratsbem.tt" wrapText="true"
		GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB310.heiratsbem.tt" />
		</tooltip>
	</TextArea>
	<Label fx:id="angelegt0" text="%Creation" GridPane.columnIndex="0" GridPane.rowIndex="5" />
	<TextField fx:id="angelegt" accessibleText="%Creation.tt"
		promptText="%Creation.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.rowIndex="5">
		<tooltip>
			<Tooltip text="%Creation.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%Change" GridPane.columnIndex="0" GridPane.rowIndex="6" />
	<TextField fx:id="geaendert" accessibleText="%Change.tt"
		promptText="%Change.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.rowIndex="6">
		<tooltip>
			<Tooltip text="%Change.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="kinder0" mnemonicParsing="true" text="%SB310.kinder" GridPane.columnIndex="0" GridPane.rowIndex="7" />
	<TableView fx:id="kinder" accessibleText="%SB310.kinder.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="7" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%SB310.kinder.tt" />
		</tooltip>
		<columns>
			<TableColumn fx:id="colUid" prefWidth="75.0" text="%SB310.colUid" />
			<TableColumn fx:id="colGeburtsname" prefWidth="150.0" text="%SB310.colGeburtsname" />
			<TableColumn fx:id="colVorname" prefWidth="150.0" text="%SB310.colVorname" />
			<TableColumn fx:id="colName" prefWidth="150.0" text="%SB310.colName" />
			<TableColumn fx:id="colGeschlecht" prefWidth="40.0" text="%SB310.colGeschlecht" />
			<TableColumn fx:id="colGa" prefWidth="170.0" text="%SB310.colGa" />
			<TableColumn fx:id="colGv" prefWidth="120.0" text="%SB310.colGv" />
			<TableColumn fx:id="colAa" prefWidth="170.0" text="%SB310.colAa" />
			<TableColumn fx:id="colAv" prefWidth="120.0" text="%SB310.colAv" />
		</columns>
	</TableView>
	<Label fx:id="kind0" mnemonicParsing="true" text="%SB310.kind" GridPane.columnIndex="0" GridPane.rowIndex="8" />
	<ComboBox fx:id="kind" accessibleText="%SB310.kind.tt" GridPane.columnIndex="1" GridPane.columnSpan="3"
		GridPane.hgrow="ALWAYS" GridPane.rowIndex="8">
		<tooltip>
			<Tooltip text="%SB310.kind.tt" />
		</tooltip>
	</ComboBox>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="hinzufuegen" mnemonicParsing="true" onAction="#onHinzufuegen" text="%SB310.hinzufuegen">
			<tooltip>
				<Tooltip text="%SB310.hinzufuegen.tt" />
			</tooltip>
		</Button>
		<Button fx:id="entfernen" mnemonicParsing="true" onAction="#onEntfernen" text="%SB310.entfernen">
			<tooltip>
				<Tooltip text="%SB310.entfernen.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
</GridPane>
