<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.cwkuehl.jhh6.app.controller.vm.VM110HausController">

	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>

	<Label fx:id="nr0" text="%VM110.nr" GridPane.columnIndex="0" />
	<TextField fx:id="nr" accessibleText="%VM110.nr.tt" promptText="%VM110.nr.tt" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%VM110.nr.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="bezeichnung0" mnemonicParsing="true" text="%VM110.bezeichnung" GridPane.columnIndex="0" GridPane.rowIndex="1" />
	<TextField fx:id="bezeichnung" accessibleText="%VM110.bezeichnung.tt" promptText="%VM110.bezeichnung.tt" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
		<tooltip>
			<Tooltip text="%VM110.bezeichnung.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="strasse0" text="%VM110.strasse" GridPane.columnIndex="0" GridPane.rowIndex="2" />
	<TextField fx:id="strasse" accessibleText="%VM110.strasse.tt" promptText="%VM110.strasse.tt" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2">
		<tooltip>
			<Tooltip text="%VM110.strasse.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="plzOrt0" text="%VM110.plzOrt" GridPane.columnIndex="0" GridPane.rowIndex="3" />
	<TextField fx:id="plz" accessibleText="%VM110.plz.tt" promptText="%VM110.plz.tt" GridPane.columnIndex="1" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%VM110.plz.tt" />
		</tooltip>
	</TextField>
	<TextField fx:id="ort" accessibleText="%VM110.ort.tt" promptText="%VM110.ort.tt" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
		<tooltip>
			<Tooltip text="%VM110.ort.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="notiz0" text="%VM110.notiz" GridPane.columnIndex="0" GridPane.rowIndex="4" />
	<TextArea fx:id="notiz" accessibleText="%VM110.notiz.tt" promptText="%VM110.notiz.tt" wrapText="true" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
		<tooltip>
			<Tooltip text="%VM110.notiz.tt" />
		</tooltip>
	</TextArea>
	<Label fx:id="angelegt0" text="%Creation" GridPane.columnIndex="0" GridPane.rowIndex="5" />
	<TextField fx:id="angelegt" accessibleText="%Creation.tt" promptText="%Creation.tt" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5">
		<tooltip>
			<Tooltip text="%Creation.tt" />
		</tooltip>
	</TextField>
	<Label fx:id="geaendert0" text="%Change" GridPane.columnIndex="0" GridPane.rowIndex="6" />
	<TextField fx:id="geaendert" accessibleText="%Change.tt" promptText="%Change.tt" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="6">
		<tooltip>
			<Tooltip text="%Change.tt" />
		</tooltip>
	</TextField>
	<HBox spacing="10.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="7">
		<Button fx:id="ok" defaultButton="true" mnemonicParsing="true" onAction="#onOk" text="%Ok">
			<tooltip>
				<Tooltip text="%Ok.tt" />
			</tooltip>
		</Button>
		<Button fx:id="abbrechen" cancelButton="true" onAction="#onAbbrechen" text="%Cancel">
			<tooltip>
				<Tooltip text="%Cancel.tt" />
			</tooltip>
		</Button>
	</HBox>

	<stylesheets>
		<URL value="@../Jhh6.css" />
	</stylesheets>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
</GridPane>
